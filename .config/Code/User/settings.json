{
  "telemetry.enableTelemetry": false,
  "extensions.ignoreRecommendations": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "vs-nomo-dark",
  "workbench.colorTheme": "Gruvbox Dark (Hard)",
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f"
  },
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.fontSize": 14,
  "editor.renderWhitespace": "boundary",
  "editor.renderIndentGuides": true,
  "editor.codeLens": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "terminal.integrated.shell.linux": "/usr/bin/fish",
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "terminal.integrated.fontSize": 14,
  "git.enableSmartCommit": true,
  "window.zoomLevel": 0,
  "npm.enableScriptExplorer": true,
  "vim.statusBarColorControl": true,
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "n"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "<leader>",
        "p"
      ],
      "after": [
        "v",
        "i",
        "w",
        "p"
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "typescript.format.enable": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": false
    }
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": false
    }
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "eslint.alwaysShowStatus": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "go.useLanguageServer": true,
  "vs-kubernetes": {
    "vs-kubernetes.kubectl-path": "/Users/onat.mercan/.vs-kubernetes/tools/kubectl/kubectl",
    "vs-kubernetes.minikube-path": "/Users/onat.mercan/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
    "vs-kubernetes.helm-path": "/Users/onat.mercan/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vs-kubernetes.draft-path": "/Users/onat.mercan/.vs-kubernetes/tools/draft/darwin-amd64/draft"
  },
  "editor.wordWrap": "on",
}